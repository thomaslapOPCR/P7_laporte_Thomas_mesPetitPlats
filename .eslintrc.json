{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-use-before-define" : "off",
    "radix": "off",
    "no-undef":"off",
    "no-param-reassign":"off",
    "indent": "off",
    "no-return-assign":"off",
    "no-shadow": "off",
    "no-plusplus": "off",
    "spaced-comment": "off",
    "max-len": "off",
    "import/extensions": "off",
    "no-case-declarations": "off",
    "eqeqeq": ["warn","always"]
  }

}
